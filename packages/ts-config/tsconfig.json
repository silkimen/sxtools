{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "plugins": [
      {
        "name": "typescript-tslint-plugin",
        "alwaysShowRuleFailuresAsWarnings": false
      }
    ]
  }
}